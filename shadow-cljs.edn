{:source-paths ["src"]
 :dependencies [[reagent "1.1.0"]]

 :dev-http     {7777 "public"}
 :nrepl        {:port 8777}

 :builds       {:app {:target :browser
                      :output-dir "public/js"
                      :asset-path "/js"
                      :modules {:main {:init-fn app.core/init}}
                      :devtools {:repl-init-ns app.core
                                 :after-load app.core/init}}}}